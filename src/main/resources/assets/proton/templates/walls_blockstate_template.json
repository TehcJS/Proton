{
  "multipart": [
    {   "when": { "up": "true" },
      "apply": { "model": "model_post" }
    },
    {   "when": { "north": "low" },
      "apply": { "model": "model_side", "uvlock": true }
    },
    {   "when": { "east": "low" },
      "apply": { "model": "model_side", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "low" },
      "apply": { "model": "model_side", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "low" },
      "apply": { "model": "model_side", "y": 270, "uvlock": true }
    },
    {   "when": { "north": "tall" },
      "apply": { "model": "model_side_tall", "uvlock": true }
    },
    {   "when": { "east": "tall" },
      "apply": { "model": "model_side_tall", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "tall" },
      "apply": { "model": "model_side_tall", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "tall" },
      "apply": { "model": "model_side_tall", "y": 270, "uvlock": true }
    }
  ]
}